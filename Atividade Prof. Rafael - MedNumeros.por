// Crie um algoritmo que leia dois números e calcule a média.

programa {
  funcao inicio() {
    //solicite 1 numero
    //solicite outro numero
    //calcule a media
    //mostre o resultado ao user

    inteiro prinumero, segnumero
    inteiro media

    escreva("Digite o primeiro número: ")
    leia(prinumero)
    escreva("Digite o segundo número: ")
    leia(segnumero)

    media = (prinumero + segnumero) / 2
    escreva("A sua média será de " + media + ".")


  }
}
